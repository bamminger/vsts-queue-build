{
    "manifestVersion": 1,
    "id": "queue-build",
    "version": "2.0.0",
    "name": "Queue Build(s) Task",
    "description": "A build/release task that queues new build(s).",
    "publisher": "jb",
    "public": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/bamminger/vsts-queue-build"
    },
    "icons": {
        "default": "images/icon_128.png"
    },
    "tags": [
        "Build",
        "Release",
        "Task"
    ],
    "scopes": [
        "vso.build_execute"
    ],
    "categories": [
        "Build and release"
    ],
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "files": [{
            "path": "src",
            "addressable": true
        },
        {
            "path": "doc",
            "addressable": true
        }
    ],
    "content": {
        "details": {
            "path": "doc/overview.md"
        },
        "license": {
            "path": "doc/license-terms.md"
        }
    },
    "contributions": [{
        "id": "queue-build-task",
        "type": "ms.vss-distributed-task.task",
        "targets": [
            "ms.vss-distributed-task.tasks"
        ],
        "properties": {
            "name": "src"
        }
    }],
    "galleryProperties": [{
        "markdownItParser": "true"
    }]
}